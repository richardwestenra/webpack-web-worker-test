{
  "name": "webpack-demo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "lib:clean": "rm -rf lib",
    "lib:copy": "cp -rf src lib",
    "lib:webpack": "webpack --config webpack.lib.js",
    "lib:babel": "babel lib --out-dir lib --copy-files",
    "lib": "npm-run-all -s lib:clean lib:copy lib:webpack lib:babel",
    "build": "webpack --config webpack.prod.js",
    "import:webpack": "webpack --config webpack.import.js",
    "import": "npm-run-all -s lib import:webpack"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "clean-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "workerize-loader": "^1.2.1"
  }
}
